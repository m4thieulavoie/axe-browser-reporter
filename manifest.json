{
  "name": "Axe Browser Reporter",
  "version": "1.0",
  "description": "Intrusive Axe accessiblity reporter",
  "manifest_version": 2,
  "permissions": ["https://*/*", "http://*/*", "storage", "tabs"],
  //   "background": { "persistent": false },
  "browser_action": { "default_popup": "popup.html" },
  // "web_accessible_resources": ["script.js"],
  "content_scripts": [{ "matches": ["https://*/*"], "js": ["script.js"] }]
}
